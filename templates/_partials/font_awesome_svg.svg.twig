<svg viewBox="{{ icon.attributes.viewBox }}" xmlns="http://www.w3.org/2000/svg">
    <style>
        @media (prefers-color-scheme: dark) {
            svg {
                fill:  white;
                color: white;
            }
        }
        @media (prefers-color-scheme: light) {
            svg {
                fill:  #3C3C3C;
                color:  #3C3C3C;
            }
        }
    </style>
    {{ icon.innerSvg|raw }}
</svg>

