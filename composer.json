{
    "name": "barth/site-src",
    "description": "Source for building my personal website",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "intervention/image": "^3.11.4",
        "knplabs/github-api": "^3.16",
        "kornrunner/blurhash": "^1.2.2",
        "league/commonmark": "^2.7.1",
        "mnapoli/front-yaml": "^2.0.4",
        "nesbot/carbon": "^3.10.3",
        "nikic/php-parser": "^5.6.2",
        "nyholm/psr7": "^1.8.2",
        "phpdocumentor/reflection-docblock": "^5.6.4",
        "ramsey/collection": "^2.1.1",
        "symfony/asset": "^7.3 || ^8.0",
        "symfony/asset-mapper": "^7.3.5 || ^8.0",
        "symfony/browser-kit": "^7.3.6 || ^8.0",
        "symfony/cache": "^7.3.6 || ^8.0",
        "symfony/console": "^7.3.6 || ^8.0",
        "symfony/css-selector": "^7.3.6 || ^8.0",
        "symfony/dom-crawler": "^7.3.3 || ^8.0",
        "symfony/dotenv": "^7.3.2 || ^8.0",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "^7.3.6 || ^8.0",
        "symfony/http-client-contracts": "^3.6.0",
        "symfony/http-foundation": "^7.3.7 || ^8.0",
        "symfony/mime": "^7.3.4 || ^8.0",
        "symfony/property-access": "^7.3.3 || ^8.0",
        "symfony/property-info": "^7.3.5 || ^8.0",
        "symfony/runtime": "^7.3.4 || ^8.0",
        "symfony/serializer": "^7.3.5 || ^8.0",
        "symfony/string": "^7.3.4 || ^8.0",
        "symfony/twig-bridge": "^7.3.6 || ^8.0",
        "symfony/twig-bundle": "^7.3.4 || ^8.0",
        "symfony/ux-icons": "^2.31",
        "symfony/ux-lazy-image": "^2.31",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "^7.3.7 || ^8.0",
        "symfony/yaml": "^7.3.5 || ^8.0",
        "symfonycasts/tailwind-bundle": "^0.11.1",
        "symfonycorp/connect": "^8.1.7",
        "thecodingmachine/safe": "^3.3",
        "twig/extra-bundle": "^3.22.1",
        "twig/intl-extra": "^3.22.1",
        "twig/markdown-extra": "^3.22",
        "twig/twig": "^3.22",
        "unsplash/unsplash": "^3.2.1"
    },
    "require-dev": {
        "ergebnis/phpstan-rules": "^2.12.0",
        "friendsofphp/php-cs-fixer": "^3.90",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "phpmd/phpmd": "^2.15",
        "phpro/grumphp": "^2.17",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.32",
        "phpstan/phpstan-strict-rules": "^2.0.7",
        "phpstan/phpstan-symfony": "^2.0.8",
        "seld/jsonlint": "^1.11",
        "squizlabs/php_codesniffer": "^4.0.1",
        "symfony/maker-bundle": "^1.65",
        "symfony/stopwatch": "^7.3.0 || ^8.0",
        "symfony/web-profiler-bundle": "^7.3.5 || ^8.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "tailwind:build --minify": "symfony-cmd",
            "asset-map:compile": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
